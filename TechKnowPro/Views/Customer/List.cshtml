@model IEnumerable<TechKnowPro.Models.Register>

@{
    ViewBag.Title = "List";
}

 
<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="card">
                <div class="card-header card-header-icon" data-background-color="purple">
                    <h4 class="card-title">Contact List</h4>
                </div>
                <div class="card-content">
                    <div class="toolbar">
                    </div>
                    <div class="material-datatables">
                        <table class="table table-striped table-no-bordered table-hover" cellspacing="0" width="100%" style="width:100%">
                            <thead>
                                <tr>
                                    <th>
                                        ProfileName
                                    </th>
                                    <th>
                                        FirstName
                                    </th>
                                    <th>
                                        LastName
                                    </th>
                                    <th>
                                        Email
                                    </th>
                                    
                                    <th>
                                        PhoneNumber
                                    </th>
                                    <th>
                                        UserName
                                    </th>
                                    <th>
                                        Address
                                    </th>
                                    <th>Actions</th>
                                </tr>
                            </thead>
                            <tbody>


                                @foreach (var item in Model)
                                {
                                <tr>
                                    <td>
                                        @item.ProfileName
                                    </td>
                                    <td>
                                        @item.FirstName
                                    </td>
                                    <td>
                                        @item.LastName
                                    </td>
                                    <td>
                                        @item.Email
                                    </td>
                                    <td>
                                        @item.PhoneNumber
                                    </td>
                                    <td>
                                        @item.UserName
                                    </td>

                                    <td>
                                        @item.Address
                                    </td>
                                    <td class="disabled-sorting text-right">
                                      @Html.ActionLink("AddContact", "AddContact", new { id = item.Id }) 
                                    </td>
                                </tr>
                                }
                            
                            </tbody>
                        </table>
                    </div>
                </div>
                <!-- end content-->
            </div>
            <!--  end card  -->
        </div>
        <!-- end col-md-12 -->
    </div>
    <!-- end row -->
</div>
 